<?xml version="1.0" encoding="UTF-8"?>
<FindBugsFilter>
    <Match>
        <!-- ignore all issues in resource generation -->
        <Class name="~.*\.R\$.*"/>
    </Match>
    <Match>
        <!-- ignore all issues in resource generation -->
        <Class name="~.*\$.*"/>
    </Match>
    <Match>
        <Class name="~.*\.Manifest\$.*"/>
    </Match>
    <Match>
        <Package name="~.*\.android"/>
    </Match>
    <Match>
        <Package name="~.*\.google*"/>
    </Match>
</FindBugsFilter>